{
	"name": "LoadingFromMongo",
	"properties": {
		"activities": [
			{
				"name": "Copy data1",
				"type": "Copy",
				"dependsOn": [],
				"policy": {
					"timeout": "0.12:00:00",
					"retry": 0,
					"retryIntervalInSeconds": 30,
					"secureOutput": false,
					"secureInput": false
				},
				"userProperties": [],
				"typeProperties": {
					"source": {
						"type": "MongoDbAtlasSource",
						"batchSize": 100
					},
					"sink": {
						"type": "DelimitedTextSink",
						"storeSettings": {
							"type": "AzureBlobFSWriteSettings"
						},
						"formatSettings": {
							"type": "DelimitedTextWriteSettings",
							"quoteAllText": true,
							"fileExtension": ".txt"
						}
					},
					"enableStaging": false,
					"translator": {
						"type": "TabularTranslator",
						"mappings": [
							{
								"source": {
									"path": "$['_id']['$oid']"
								},
								"sink": {
									"name": "$oid"
								}
							},
							{
								"source": {
									"path": "$['PID']['$numberLong']"
								},
								"sink": {
									"name": "PID_$numberLong"
								}
							},
							{
								"source": {
									"path": "$['Pool Area']['$numberLong']"
								},
								"sink": {
									"name": "Pool Area_$numberLong"
								}
							},
							{
								"source": {
									"path": "$['Pool QC']"
								},
								"sink": {
									"name": "Pool QC"
								}
							}
						]
					}
				},
				"inputs": [
					{
						"referenceName": "MongoDb_Pool",
						"type": "DatasetReference"
					}
				],
				"outputs": [
					{
						"referenceName": "PoolCSV",
						"type": "DatasetReference"
					}
				]
			}
		],
		"annotations": []
	}
}